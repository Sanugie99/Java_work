package ex01_for;

public class Ex02_for {

	public static void main(String[] args) {
		/*
		 * for문안에 또 다른 for문을 사용하는 경우
		 * 반복문을 반족하는 것
		 * for(초기식; 조건식; 증감식){
		 * 	for(초기식; 조건식; 증감식){
		 * 		반복하고자 하는 명령
		 * 	}
		 * }
		 */
		
		for(int i = 0; i < 3; i++) {
			for(int j = 0; j < 3; j++) {
				System.out.println(i+" "+j);
			}
		}
		// A B C D
		// E F G H
		// I J K L
		char ch = 'A';
		for(int i = 0; i <= 2; i++) {
			for(int j = 0; j <= 3; j++) {
				System.out.print(ch);
				++ch;
			}
			System.out.println();
		}

	}

}
