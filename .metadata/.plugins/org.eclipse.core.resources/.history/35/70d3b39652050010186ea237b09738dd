package ex04_casting;

public class Ex03_casting {
	public static void main(String[] args) {
		//float -> int 강제형변환
		float f1 = 12345.67f;
		int i1 = (int)f1;
		System.out.println("[float -> int] f1의 값 : " + f1 + ", i1의 값 : " + i1);
		
		//double -> int로 강제형변환
		double d1 = 12345.67;
		int i2 = (int)d1;
		System.out.println("[double -> int] d1의 값 : " + d1 + ", i2의 값 : " + i2);
	}
}
