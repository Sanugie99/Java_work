package ex03_Polymorphism;

public class ComputerRoom {
	
	Samsung com1;
	Samsung com2;
	
	public void allPowerOn() {
		com1.powerOn();
		com2.powerOn();
	}
	
	public void allPowerOff() {
		com1.powerOff();
		com2.powerOff();
	}
	
//	public static void main(String[] args) {
//		ComputerRoom cr = new ComputerRoom();
//		System.out.println(name);
//	}
	
	
}
