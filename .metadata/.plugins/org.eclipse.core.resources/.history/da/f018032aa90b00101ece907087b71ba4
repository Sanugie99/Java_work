package ex04_api;

public class Ex03_String {
	public static void main(String[] args) {
		String str = "Kim Mal Ddong";
		
		//1. lenght()
		//문자열의 길이를 반환하는 메서드
		System.out.println("문자열 str의 길이 : " + str.length());
		
		//2. indextOf(char ch)
		//인자로 전달된 글자의 인덱스 값을 반환한다.
		System.out.println("글자 M의 위치 : " + str.indexOf('m'));
	}
}
